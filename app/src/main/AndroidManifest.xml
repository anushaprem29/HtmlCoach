<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anusha.htmlcoach" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name=".LogIn"
            android:label="@string/title_activity_log_in" >
        </activity>
        <activity
            android:name=".Welcome"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name=".Heading"
            android:label="@string/title_activity_heading" >
        </activity>
        <activity
            android:name=".Styles"
            android:label="@string/title_activity_styles" >
        </activity>
        <activity
            android:name=".CSS"
            android:label="@string/title_activity_css" >
        </activity>
        <activity
            android:name=".Links"
            android:label="@string/title_activity_links" >
        </activity>
        <activity
            android:name=".Tables"
            android:label="@string/title_activity_tables" >
        </activity>
        <activity
            android:name=".Images"
            android:label="@string/title_activity_images" >
        </activity>
    </application>

</manifest>
